{
    "extensions.ignoreRecommendations": false,
    "workbench.iconTheme": "vs-minimal",
    "workbench.colorCustomizations": {
        "[Default Light+]": {
            "titleBar.activeBackground": "#252526",
            "titleBar.activeForeground": "#CCC",
        }
    },
    "editor.minimap.enabled": false,
    "git.autofetch": true,
    "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
    "files.autoSave": "onFocusChange",
    "files.defaultLanguage": "markdown",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": false,
    "prettier.singleQuote": true,
    "azure.resourceFilter": [
        "b196ecbd-2dd0-4616-8f71-aae4492dd2e6/4d106790-62cc-4e59-8ec0-cec5344d206f"
    ],
    "workbench.statusBar.visible": true,
    "workbench.activityBar.visible": true,
    "window.zoomLevel": 0,
    "cSpell.userWords": [
        "Spotify",
        "lightbend",
        "reproducibility"
    ],
    "cSpell.logLevel": "None",
    "cSpell.showStatus": false,
    "wordcounter.count_lines": false,
    "wordcounter.readtime": false,
    "window.restoreWindows": "none",
    "window.newWindowDimensions": "maximized",
    "cSpell.allowCompoundWords": true,
    "markdownConverter.Document.HeaderFooterEnabled": false,
    "markdownConverter.Document.Paper.Margin": {
        "Top": "0",
        "Bottom": "0",
        "Left": "0",
        "Right": "0"
    },
    "typescript.updateImportsOnFileMove.enabled": "always",
    "breadcrumbs.enabled": false,
    "git.promptToSaveFilesBeforeCommit": true,
    "editor.fontFamily": "Consolas Ligaturized, Consolas, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "window.menuBarVisibility": "toggle",
    "window.title": "${rootName}${separator}${dirty}${activeEditorMedium}",
    "zenMode.fullScreen": false,
    "zenMode.centerLayout": false,
    "markdownConverter.DestinationOrigin": "DocumentFile",
    "markdown.preview.fontFamily": "\"Inter UI\", -apple-system, BlinkMacSystemFont, 'Segoe WPC', 'Segoe UI', 'HelveticaNeue-Light', 'Ubuntu', 'Droid Sans', sans-serif",
    "markdown.preview.scrollEditorWithPreview": false,
    "liveServer.settings.donotShowInfoMsg": true,
    "window.openFilesInNewWindow": "default",
    "workbench.panel.defaultLocation": "right",
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "terminal.integrated.rendererType": "canvas",
    "emojisense.markupCompletionsEnabled": false,
    "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "git.detectSubmodulesLimit": 20,
    "gitlens.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "[javascriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "terminal.integrated.fontFamily": "Cascadia Code",
    "workbench.colorTheme": "Default Light+",
    "diffEditor.renderSideBySide": false,
    "editor.renderWhitespace": "none",
    "debug.onTaskErrors": "showErrors",
    "diffEditor.ignoreTrimWhitespace": false,
    "remote.SSH.remotePlatform": {
        "samaritan": "linux",
        "samaritanl": "linux"
    },
    "cSpell.enabledLanguageIds": [
        "asciidoc",
        "c",
        "cpp",
        "css",
        "git-commit",
        "go",
        "graphql",
        "handlebars",
        "haskell",
        "html",
        "jade",
        "java",
        "javascript",
        "javascriptreact",
        "json",
        "jsonc",
        "latex",
        "less",
        "markdown",
        "php",
        "plaintext",
        "python",
        "pug",
        "restructuredtext",
        "rust",
        "scala",
        "scss",
        "text",
        "typescript",
        "typescriptreact",
        "yaml",
        "yml"
    ],
    "dotfiles.targetPath": "~/.dotfiles",
    "dotfiles.installCommand": "~/.dotfiles/install.sh",
    "dotfiles.repository": "mjwsteenbergen/dotfiles",
}